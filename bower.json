{
  "name": "angular-seed",
  "version": "1.0.0",
  "homepage": "https://github.com/thiren/angular-seed",
  "authors": [
    "Thiren Bunsee <thirenbunsee@gmail.com>"
  ],
  "description": "Seed for new Angular applications",
  "main": "index.html",
