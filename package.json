{
  "name": "angular-seed",
  "version": "0.0.0",
  "description": "A project seed for new angular 1.x applications",
  "main": "index.html",
  "scripts": {
    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "gulp watch --env=dev",
    "pretest": "npm install | gulp constants --env=test | gulp html --env=test",
    "test": "karma start karma.conf.js",
